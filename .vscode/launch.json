	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "dev/ntun",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/development/ntun.development.js",
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "dev/ntun.cli",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/development/ntun.cli.development.js",
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "dev/webrtc",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/development/webrtc/WebRTCTransport.development.js",
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "dev/vk-webrtc",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/development/vk-webrtc/vk.development.js",
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "ntun.cli/client",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/node.cli.js",
			"console": "integratedTerminal",
			"args": [
				"-i",
				"8080",
				"--transport",
				"tcp",
				"localhost:8081"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "ntun.cli/server",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/node.cli.js",
			"console": "integratedTerminal",
			"args": [
				"-o",
				"-t",
				"tcp",
				"8081"
			]
		}
	]
}